#!/usr/bin/env python
PACKAGE = "velocity_controller"

from dynamic_reconfigure.parameter_generator_catkin import *

gen = ParameterGenerator()

#gen.add(name, type, level, description, default, min, max)

gen.add("VctrlMax_X", double_t, 0, "MaxSpeed of X m/s", 10.0, 0.1, 30.0)
gen.add("VctrlMax_Y", double_t, 0, "MaxSpeed of Y m/s", 10.0, 0.1, 30.0)
gen.add("VctrlMax_Yaw", double_t, 0, "MaxSpeed of Yaw rad/s", 10.0, 0.1, 30.0)

gen.add("TdiffMax_X", double_t, 0, "Max Rate of Thrust Differentiation of X 1/100%", 0.1, 0.01, 1.0)
gen.add("TdiffMax_Y", double_t, 0, "Max Rate of Thrust Differentiation of Y 1/100%", 0.1, 0.01, 1.0)
gen.add("TdiffMax_Yaw", double_t, 0, "Max Rate of Thrust Differentiation of Yaw 1/100%", 0.1, 0.01, 1.0)

gen.add("PGain_X", double_t, 0, "P Gain of X", 1.0)
gen.add("PGain_Y", double_t, 0, "P Gain of Y", 1.0)
gen.add("PGain_Yaw", double_t, 0, "P Gain of Yaw", 1.0)


exit( gen.generate(PACKAGE, "velocity_controller_node", "Constants") )
